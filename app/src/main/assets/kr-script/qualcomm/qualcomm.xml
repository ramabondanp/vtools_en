    <group title="LLCC" id="LLCC" visible="run llcc.sh visible">
        <picker reload="LLCC" options-sh="run llcc.sh options" shell="hidden">
            <title>Max Freq</title>
            <get>run llcc.sh get_max_freq</get>
            <set>run llcc.sh set_max_freq</set>
